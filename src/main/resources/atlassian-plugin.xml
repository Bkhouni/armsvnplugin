	  <label key="Subversion ALM"/>
	  <tooltip key="10 latest commits"/>
	  <resource type="download" name="icon.png" location="/svnwebclient/images/subversion-jira-integration_16.png"/>
	  <description>Shows Subversion commits on the Agile boards</description>
	</web-panel>

