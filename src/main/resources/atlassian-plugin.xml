	<resource
		location="com.kintosoft.jira.plugin.ext.subversion.action.SubversionActionSupport"
		name="ALMSubversionActionSupport" type="i18n" />
	<!-- this property file contains i18n keys for the panel -->
	<resource
		location="com.kintosoft.templates.plugins.subversion.projecttabpanels.subversion_jira_plugin"
		name="alm-projecttabpanels-subversion_jira_plugin" type="i18n" />
	<resource
		location="com.kintosoft.templates.plugins.subversion.issuetabpanels.subversion_jira_plugin"
		name="alm-issuetabpanels-subversion_jira_plugin" type="i18n" />
	
	<issue-tabpanel
		class="com.kintosoft.jira.plugin.ext.subversion.issuetabpanels.changes.SubversionRevisionsTabPanel"
		key="svnplus-subversion-commits-tabpanel" name="Subversion ALM Commits Tab Panel">
		<description>It shows Subversion commits related to an issue in an
			issue tab panel.
		</description>
		<label key="subversion.issue-tab.label" />
		<resource
			location="/com/kintosoft/templates/plugins/subversion/issuetabpanels/subversion-commits-view.vm"
			name="view" type="velocity" />
		<supports-ajax-load>true</supports-ajax-load>
	</issue-tabpanel>
	<project-tabpanel
		class="com.kintosoft.jira.plugin.ext.subversion.projecttabpanels.SubversionProjectTabPanel"
		key="svnplus-subversion-project-tab" name="Subversion ALM Commits Project Tab">
		<description key="subversion.project-tab.description" />
		<label key="subversion.project-tab.label" />
		<!-- this is a number defining the order of all project tabs. The system 
			panels are 10, 20, 30 and 40. -->
		<order>50</order>
		<!-- this template produces the HTML for the panel -->
		<resource
			location="/com/kintosoft/templates/plugins/subversion/projecttabpanels/subversion-project-tab.vm"
			name="view" type="velocity" />
	</project-tabpanel>

