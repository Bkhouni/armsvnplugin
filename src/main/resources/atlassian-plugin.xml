				</view>
				<view name="permissionviolation">/com/kintosoft/templates/plugins/subversion/configuration/no-privilege.vm
				</view>
				<view name="error">/com/kintosoft/templates/plugins/subversion/configuration/delete.vm
				</view>
			</action>
