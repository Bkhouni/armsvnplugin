	<servlet name="DWRServlet" key="org.directwebremoting.servlet.DwrServlet"
		class="org.directwebremoting.servlet.DwrServlet">
		<description key="svnwebcient.dwr.servlet">DWR Servlet</description>
		<url-pattern>/svnwebclient/dwr/*</url-pattern>
		<init-param>
			<param-name>debug</param-name>
			<param-value>false</param-value>
		</init-param>
		<init-param>
			<param-name>skipDefaultConfig</param-name>
			<param-value>true</param-value>
		</init-param>
		<init-param>
			<param-name>classes</param-name>
			<param-value>com.kintosoft.svnwebclient.graph.db.StatsService,
				com.kintosoft.svnwebclient.graph.db.CommitGraphService,
				com.kintosoft.svnwebclient.graph.model.ItemModel,
				com.kintosoft.svnwebclient.graph.model.RevisionModel,
				com.kintosoft.svnwebclient.graph.model.ActionModel,
				com.kintosoft.svnwebclient.graph.model.CopyModel,
				com.kintosoft.svnwebclient.graph.model.Graph,
				com.kintosoft.svnwebclient.graph.model.Segment,
				com.kintosoft.svnwebclient.graph.db.SegmentCommitsRequest,
				com.kintosoft.svnwebclient.graph.model.jira.Issue
			</param-value>
		</init-param>
		<init-param>
			<param-name>generateDtoClasses</param-name>
			<param-value>interface</param-value>
		</init-param>
	</servlet>
	<jql-function name="svnMultiCommit" key="svnHasCommits"
		class="com.kintosoft.jira.jql.SVNMultiCommit">
		<description>JQL Function: It returns issues having a minimal amount
			commits
		</description>
	</jql-function>
	<jql-function name="svnMultiProject" key="svnMultiProject"
		class="com.kintosoft.jira.jql.SVNMultiProject">
		<description>JQL Function: It returns issues with cross project
			commits
		</description>
	</jql-function>
	<jql-function name="svnMultiAuthor" key="svnMultiAuthor"
		class="com.kintosoft.jira.jql.SVNMultiAuthor">
		<description>JQL Function: It returns issues with more than one
			contributor
		</description>
	</jql-function>
	<jql-function name="svnMultiRepository" key="svnMultiRepository"
		class="com.kintosoft.jira.jql.SVNMultiRepository">
		<description>JQL Function: It returns issues with commits belonging to
			more than one repository
		</description>
	</jql-function>
	<jql-function name="svnAuthor" key="svn-commit-author-is"
		class="com.kintosoft.jira.jql.SVNCommitAuthorIs">
		<description>JQL Function: It returns the issues related to commits
			made for an specific author
		</description>
	</jql-function>
	<jql-function name="svnItem" key="svn-item"
		class="com.kintosoft.jira.jql.SVNItem">
		<description>JQL Function: It gets files or directories related to an
			issue
		</description>
	</jql-function>
	<jql-function name="svnComment" key="svn-tex-in-comment"
		class="com.kintosoft.jira.jql.SVNTexInComment">
		<description>JQL Function: It returns issues related to commits
			containing the text in their commit comment messages
		</description>
	</jql-function>
	<jql-function name="svnCommitNumberRange" key="svn-commits-in-range"
		class="com.kintosoft.jira.jql.SVNCommitsInRange">
		<description>JQL Function: It returns the issues related to commits
			between a revision range
		</description>
	</jql-function>
	<jql-function name="svnCommitDateRange" key="svn-commits-in-date-range"
		class="com.kintosoft.jira.jql.SVNCommitsInDateRange">
		<description>JQL Function: It returns the issues ralated to commits
			between a date range
		</description>
	</jql-function>

