				</view>
				<view name="error">/com/kintosoft/templates/plugins/subversion/configuration/add.vm
				</view>
				<view name="permissionviolation">/com/kintosoft/templates/plugins/subversion/configuration/no-privilege.vm
				</view>
			</action>
			<action alias="ALMUpdateSubversionRepository"
				name="com.kintosoft.jira.plugin.ext.subversion.action.UpdateSubversionRepositoryAction">
				<view name="input">/com/kintosoft/templates/plugins/subversion/configuration/edit.vm
				</view>
				<view name="permissionviolation">/com/kintosoft/templates/plugins/subversion/configuration/no-privilege.vm
				</view>
				<view name="error">/com/kintosoft/templates/plugins/subversion/configuration/edit.vm
