<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="${atlassian.plugin.key}" name="${project.name}"
	plugins-version="2">
	<plugin-info>
		<description>${project.description}</description>
		<vendor name="${project.organization.name}" url="${project.organization.url}" />
		<version>${project.version}</version>
		<application-version min="${atlassian.plugin.application.version.min}" />
		<param name="atlassian-licensing-enabled">false</param>
		<param name="plugin-icon">svnwebclient/images/subversion-jira-integration_16.png</param>
		<param name="plugin-logo">svnwebclient/images/subversion-jira-integration_72.png</param>
		<param name="vendor-icon">svnwebclient/images/kintosoft_16.png</param>
		<param name="vendor-logo">svnwebclient/images/kintosoft_72.png</param>
		<param name="configure.url">/secure/ALMViewSubversionRepositories.jspa</param>
	</plugin-info>

	<ao key="ao-module">
		<description>The module configuring the Active Objects service used by this plugin</description>
		<entity>com.kintosoft.svnwebclient.graph.entities.ao.Action</entity>
		<entity>com.kintosoft.svnwebclient.graph.entities.ao.AppConfig</entity>
		<entity>com.kintosoft.svnwebclient.graph.entities.ao.Comment</entity>
		<entity>com.kintosoft.svnwebclient.graph.entities.ao.Copy</entity>
		<entity>com.kintosoft.svnwebclient.graph.entities.ao.Item</entity>
		<entity>com.kintosoft.svnwebclient.graph.entities.ao.Key</entity>
		<entity>com.kintosoft.svnwebclient.graph.entities.ao.Repository</entity>
		<entity>com.kintosoft.svnwebclient.graph.entities.ao.RepoConfig</entity>
		<entity>com.kintosoft.svnwebclient.graph.entities.ao.Revision</entity>
	</ao>

