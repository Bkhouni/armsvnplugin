		<resource location="svnwebclient/" name="svnwebclient/" type="download" />
	</web-resource>
	<!-- Reports INI -->
	<report name="Subversion ALM Filter Report"
		key="svn-plus-filter-reports" class="com.kintosoft.jira.reports.SVNFilterReport">
		<description>It shows the commits calendar view for the iussues of a given filter</description>
		<resource name="view" type="velocity"
			location="/com/kintosoft/templates/plugins/svnwebclient/reports/commits-calendar-view.vm" />
		<label>Commit Calendar View</label>
		<properties>
			<property>
				<key>filterId</key>
				<name>Filter</name>
				<description></description>
				<type>filterpicker</type>
				<values class="com.atlassian.jira.portal.SearchRequestValuesGenerator" />
			</property>
		</properties>
	</report>
	<!-- Repoprts END -->
	<servlet-filter name="SVN Web Client Forward Filter"
		i18n-name-key="svn-web-client-forward-filter.name" key="svn-web-client-forward-filter"
		class="com.kintosoft.jira.servlet.SVNWebClientForwardFilter" location="before-dispatch"
		weight="1000000">
		<description key="svn-web-client-forward-filter.description">The SVN Web Client Forward Filter Plugin
		</description>
		<url-pattern>/plugins/servlet/svnwebclient/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
	</servlet-filter>
	<servlet-filter name="Database Web Console Access Filter"
		key="svn-web-client-db-console-access-filter"
		class="com.kintosoft.jira.servlet.DBConsoleAuthorizationFilter" location="before-dispatch"
		weight="999999">
		<description>This filter ensures that only JIRA administrators can access to the database web console
		</description>
		<url-pattern>/plugins/servlet/svnwebclient/console/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
	</servlet-filter>
	
