	<web-item key="svnplus-svn_topmenu_link" name="Show Subversion on the JIRA top menu"
		section="system.top.navigation.bar" weight="47">
		<label>Subversion ALM</label>
		<link linkId="svnplus-svn_topmenu_link">/secure/SWCTopMenuAction!default.jspa</link>
